{
  "name": "picapp",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:commit": "ng test --watch=false --browsers=ChromeHeadless",
    "report:generate": "npx lighthouse http://127.0.0.1:8080/ --output html --output-path ./reports/index.html",
    "report:open": "npx http-server reports/ -p 0 -o",
    "report": "npm-run-all report:generate report:open",
    "prepare": "husky install",
    "pwa": "npx http-server dist/picapp/ -p 0 -o"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.1.0",
    "@angular/common": "~13.1.0",
    "@angular/compiler": "~16.0.0",
    "@angular/core": "~13.1.0",
    "@angular/forms": "~13.1.0",
    "@angular/platform-browser": "~13.1.0",
    "@angular/platform-browser-dynamic": "~13.1.0",
    "@angular/router": "~13.1.0",
    "@angular/service-worker": "~13.1.0",
    "rxjs": "~7.8.1",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~16.0.0",
    "@angular/cli": "~13.1.2",
    "@angular/compiler-cli": "~13.1.0",
    "@popperjs/core": "^2.11.0",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^20.1.4",
    "animate.css": "^4.1.1",
    "bootstrap": "^5.1.3",
    "bootstrap-icons": "^1.7.2",
    "http-server": "^14.1.1",
    "husky": "^7.0.4",
    "jasmine-core": "~4.6.0",
    "jquery": "^3.6.0",
    "karma": "~6.3.16",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "lighthouse": "^9.6.8",
    "npm-run-all": "^4.1.5",
    "typescript": "~4.5.2"
  }
}
